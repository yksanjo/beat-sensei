# Beat-Sensei Environment Variables

# Soundraw API token for AI music generation (required for generation)
# Get yours at: https://soundraw.io
SOUNDRAW_API_TOKEN=

# DeepSeek API key for chatbot (recommended - cheap and fast!)
# Get yours at: https://platform.deepseek.com/api_keys
DEEPSEEK_API_KEY=

# Replicate API token for AI sample generation (Pro feature)
# Get yours at: https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=

# OpenAI API key for chatbot (alternative to DeepSeek)
# Get yours at: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Supabase for sample library (optional but recommended)
# Get these from your Supabase project settings > API
SUPABASE_URL=
SUPABASE_ANON_KEY=

# For uploading samples (admin only)
SUPABASE_SERVICE_KEY=
